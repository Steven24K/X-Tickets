{
  "kind": "collectionType",
  "collectionName": "forms",
  "info": {
    "singularName": "form",
    "pluralName": "forms",
    "displayName": "Form"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Fields": {
      "type": "dynamiczone",
      "components": [
        "form-fields.time-select",
        "form-fields.text",
        "form-fields.text-area",
        "form-fields.password",
        "form-fields.number",
        "form-fields.info",
        "form-fields.email",
        "form-fields.drop-down",
        "form-fields.date-picker",
        "form-fields.check-box",
        "form-fields.hidden",
        "form-fields.file",
        "form-fields.date-time-list",
        "form-fields.venue-field"
      ],
      "required": true,
      "min": 1
    },
    "SubmitButton": {
      "type": "string",
      "default": "Submit",
      "required": true
    },
    "SubmitText": {
      "type": "text",
      "default": "Thanks for filling in the form.",
      "required": true
    },
    "SubmitAction": {
      "type": "string",
      "default": "/api/submit/",
      "required": true
    },
    "RedirectUrl": {
      "type": "string"
    }
  }
}
