{
  "kind": "collectionType",
  "collectionName": "shop_settings",
  "info": {
    "singularName": "shop-setting",
    "pluralName": "shop-settings",
    "displayName": "ShopSetting"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "OptionName": {
      "type": "uid",
      "required": true
    },
    "isNumber": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "OptionValue": {
      "type": "string",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "isNumber"
            },
            false
          ]
        }
      },
      "required": false
    },
    "NumericOption": {
      "type": "integer",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "isNumber"
            },
            true
          ]
        }
      }
    },
    "Owner": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "Setting"
    }
  }
}
