{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "text",
      "maxLength": 120
    },
    "slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    },
    "Blocks": {
      "type": "dynamiczone",
      "components": [
        "blocks.text-block",
        "blocks.hero",
        "blocks.image",
        "blocks.overview",
        "blocks.form",
        "blocks.authorize"
      ],
      "required": true,
      "min": 1
    }
  }
}
